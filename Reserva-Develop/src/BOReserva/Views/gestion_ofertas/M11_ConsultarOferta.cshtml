<!-----------------SECCION CONSULTAR OFERTA-------------------->
<div class="row">
    <div class="col-md-12">
        <div class="box box">
            <div class="box-header with-border">
                <h1 class="box-title"> Consultar Oferta</h1>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                        <i class="fa fa-minus"></i>
                    </button>
                </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <div class="row">

                    <div class="col-md-12">
                        <div class="col-xs-4">
                            <label>Seleccione Oferta</label>
                            <select class="form-control" id="cbConOfeSelecOferta">
                                <option>Seleccionar Oferta</option>
                            </select>
                        </div>
                    </div>

                    <div class="col-md-12" style="margin-top: 25px">

                        <div class="col-xs-4">
                            <label>Nombre de la Oferta</label>
                            <input type="text" class="form-control" id="iConNombreOferta" disabled="">
                        </div>

                        <div class="col-xs-3">
                            <label>Fecha Inicio:</label>

                            <input type="text" class="form-control" id="iConOfeFechaIn" placeholder="dd/mm/yyyy" disabled="">

                        </div>

                        <div class="col-xs-3">
                            <label>Fecha Fin:</label>

                            <input type="text" class="form-control" id="iConOfeFechaFin" placeholder="dd/mm/yyyy" disabled="">

                        </div>

                    </div>

                    <div class="col-md-10" style="margin-bottom: 25px; margin-top: 25px">

                        <div class="col-xs-4">
                            <label>Porcentaje de Oferta</label>
                            <input type="text" class="form-control" id="iPrecioOferta" disabled="">
                        </div>

                        <div class="col-xs-4">
                            <label>Estado de la Oferta</label>
                            <input type="text" class="form-control" id="iConEstOfe" disabled="">
                        </div>
                    </div>
                    <!-- /.col -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
    <!-- /.col -->
</div>
<!-----------------FIN SECCION CONSULTAR OFERTA-------------------->
